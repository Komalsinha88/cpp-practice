#include<iostream>
using namespace std;
int main(){
    int n, i, arr[n];
    cout<<"Enter the size of an array "<<endl;
    cin>>n;

    cout<<"Enter the elements of an array "<<endl;
    for(i=0; i<n; i++){
        cin>>i;
    }

    if(arr[i] % 2 == 0){
        cout<<"0"<<endl;
    }else{
        cout<<"1"<<endl;
    }
    return 0;
}